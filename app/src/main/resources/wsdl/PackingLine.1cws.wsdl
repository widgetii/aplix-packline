			</input>
			<output>
				<soapbind:body use="literal" />
			</output>
		</operation>
		<operation name="AddContainer">
			<soapbind:operation soapAction="http://www.aplix.ru/PackingLine/1.0/ws/AddContainer" />
			<input>
