		<xsd:schema>
			<xsd:import namespace="http://www.aplix.ru/PackingLine/1.0/ws/model" schemaLocation="PackingLine.1cws_schema1.xsd" />
		</xsd:schema>
		<xsd:schema>
			<xsd:import namespace="http://www.aplix.ru/PackingLine/1.0/ws/messages" schemaLocation="PackingLine.1cws_schema2.xsd" />
		</xsd:schema>
	</types>

