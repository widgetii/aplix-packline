<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://schemas.xmlsoap.org/wsdl/" xmlns:soapbind="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="http://www.aplix.ru/PackingLine/1.0/ws"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:model="http://www.aplix.ru/PackingLine/1.0/ws/model" xmlns:msg="http://www.aplix.ru/PackingLine/1.0/ws/messages"
	name="PackingLine" targetNamespace="http://www.aplix.ru/PackingLine/1.0/ws">
